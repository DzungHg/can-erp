            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Property="Credit_Account" Title="Credit Account" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Property="Description" Title="Description" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Format="decimal" Property="Amount" Title="Amount" Type="number">
