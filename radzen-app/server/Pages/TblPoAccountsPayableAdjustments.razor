            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Property="Record_Code" Title="Record Code" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Property="PO_No" Title="PO No" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Property="Invoice_No" Title="Invoice No" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Format="date-time" Property="Invoice_Date" FormatString="{0:MM/dd/yyyy}" Title="Invoice Date" Type="string">
