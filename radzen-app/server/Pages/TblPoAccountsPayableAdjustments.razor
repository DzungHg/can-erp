        <RadzenGrid @ref="grid0" AllowFiltering="true" AllowPaging="true" AllowSorting="true" Data="@(getTblPoAccountsPayableAdjustmentsResult)" TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" RowSelect="@Grid0RowSelect">
          <Columns>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblPoAccountsPayableAdjustment" Context="erpCanModelsCanErpDbAt132TblPoAccountsPayableAdjustment" Property="Voucher_No" Title="Voucher No" Type="string">
