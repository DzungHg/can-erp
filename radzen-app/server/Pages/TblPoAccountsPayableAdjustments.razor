@page "/tbl-po-accounts-payable-adjustments"
@layout MainLayout
@inherits ErpCan.Pages.TblPoAccountsPayableAdjustmentsComponent

@using Radzen
@using Radzen.Blazor
@using ErpCan.Models.CanErpDbAt132
<RadzenContent Container="contentContainer0">
