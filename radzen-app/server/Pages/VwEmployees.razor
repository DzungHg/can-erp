    <RadzenHeading Size="H1" Text="Vw Employees">
    </RadzenHeading>
    <div class="row">
      <div class="col-md-12">
        <RadzenGrid @ref="grid0" AllowFiltering="true" AllowPaging="true" AllowSorting="true" Data="@(getVwEmployeesResult)" TItem="ErpCan.Models.CanErpDbAt132.VwEmployee">
          <Columns>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="AddressBook_SEQ" Title="Address Book SEQ">
