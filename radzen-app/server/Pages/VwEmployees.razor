            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="LastName" Title="Last Name">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="FirstName" Title="First Name">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="Gender_FK" Title="Gender FK">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="DOB" FormatString="{0:MM/dd/yyyy}" Title="DOB">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="Origin" Title="Origin">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="IDNumber" Title="ID Number">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="TaxIDNumber" Title="Tax ID Number">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="OrganizationName" Title="Organization Name">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="JobPosition" Title="Job Position">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="Address" Title="Address">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.VwEmployee" Context="erpCanModelsCanErpDbAt132VwEmployee" Property="City_FK" Title="City FK">
            </RadzenGridColumn>
          </Columns>
        </RadzenGrid>
      </div>
    </div>
  </ChildContent>
</RadzenContent>
