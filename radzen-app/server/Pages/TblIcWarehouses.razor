@page "/tbl-ic-warehouses"
@layout MainLayout
@inherits ErpCan.Pages.TblIcWarehousesComponent

@using Radzen
@using Radzen.Blazor
@using ErpCan.Models.CanErpDbAt132
<RadzenContent Container="contentContainer0">
  <ChildContent>
    <RadzenHeading Size="H1" Text="Tbl Ic Warehouses">
    </RadzenHeading>
    <div class="row">
      <div class="col-md-12">
        <RadzenButton Icon="add_circle_outline" style="margin-bottom: 10px" Text="Add" Click="@Button0Click">
        </RadzenButton>
        <RadzenGrid @ref="grid0" AllowFiltering="true" AllowPaging="true" AllowSorting="true" Data="@(getTblIcWarehousesResult)" TItem="ErpCan.Models.CanErpDbAt132.TblIcWarehouse" RowSelect="@Grid0RowSelect">
          <Columns>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblIcWarehouse" Context="erpCanModelsCanErpDbAt132TblIcWarehouse" Property="Warehouse_ID" Title="Warehouse ID" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblIcWarehouse" Context="erpCanModelsCanErpDbAt132TblIcWarehouse" Property="WarehouseName" Title="Warehouse Name" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblIcWarehouse" Context="erpCanModelsCanErpDbAt132TblIcWarehouse" Property="WarehouseAddress" Title="Warehouse Address" Type="string">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblIcWarehouse" Context="erpCanModelsCanErpDbAt132TblIcWarehouse" Property="Inactive" Title="Inactive" Type="boolean">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="ErpCan.Models.CanErpDbAt132.TblIcWarehouse" Context="erpCanModelsCanErpDbAt132TblIcWarehouse" Filterable="false" Sortable="false" Bubble="false" Width="70px" TextAlign="TextAlign.Center">
              <Template Context="erpCanModelsCanErpDbAt132TblIcWarehouse">
                <RadzenButton ButtonStyle="ButtonStyle.Danger" Icon="close" Size="ButtonSize.Small" Click="@((args) =>GridDeleteButtonClick(args, erpCanModelsCanErpDbAt132TblIcWarehouse))">
                </RadzenButton>
              </Template>
            </RadzenGridColumn>
          </Columns>
        </RadzenGrid>
      </div>
    </div>
  </ChildContent>
</RadzenContent>
