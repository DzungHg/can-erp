{
  "components": [
    {
      "type": "content",
      "container": "contentContainer0",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Tbl PO List"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-tbl-po-list",
                        "type": "popup",
                        "then": [
                          {
                            "code": "grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getTblPoPurchaseOrdersResult}",
                  "events": {
                    "loadData": [],
                    "rowDoubleClick": [
                      {
                        "parameters": [
                          {
                            "name": "PO_ID",
                            "value": "${event.PO_ID}"
                          }
                        ],
                        "path": "edit-tbl-po-list",
                        "type": "popup"
                      }
                    ],
                    "rowSelect": [
                      {
                        "name": "master",
                        "type": "set",
                        "value": "${event}",
                        "valueType": "ErpCan.Models.CanErpDbAt132.TblPoPurchaseOrder"
                      },
                      {
                        "name": "CanERP_DB_AT132.getTblPoPurchaseOrderDetails",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "i => i.PO_FK == '${event.PO_ID}'"
                          }
                        ],
                        "then": [
                          {
                            "name": "TblPoPurchaseOrderDetails",
                            "type": "set",
                            "value": "${result}",
                            "valueType": "IEnumerable<ErpCan.Models.CanErpDbAt132.TblPoPurchaseOrderDetail>"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "PO_ID",
                      "title": "PO ID"
                    },
                    {
                      "property": "PODate",
                      "formatString": "{0:d}",
                      "title": "PO Date"
                    },
                    {
                      "property": "CreatedBy_FK",
                      "sortProperty": "TblGnAddressBook.AddressBook_ID",
                      "filterProperty": "TblGnAddressBook.AddressBook_ID",
                      "template": "${data.TblGnAddressBook?.AddressBook_ID}",
                      "title": "Tbl Gn Address Book"
                    },
                    {
                      "property": "POStatus_FK",
                      "sortProperty": "TblPoOrderStatus.POStatusText",
                      "filterProperty": "TblPoOrderStatus.POStatusText",
                      "template": "${data.TblPoOrderStatus?.POStatusText}",
                      "title": "Tbl Po Order Status"
                    },
                    {
                      "property": "Vendor_FK",
                      "sortProperty": "TblPoVendor.LastName",
                      "filterProperty": "TblPoVendor.LastName",
                      "template": "${data.TblPoVendor?.LastName}",
                      "title": "Tbl Po Vendor"
                    },
                    {
                      "property": "Description",
                      "title": "Description"
                    },
                    {
                      "property": "Incoterm_FK",
                      "sortProperty": "TblGnIncoterm.Incoterm_Name",
                      "filterProperty": "TblGnIncoterm.Incoterm_Name",
                      "template": "${data.TblGnIncoterm?.Incoterm_Name}",
                      "title": "Tbl Gn Incoterm"
                    },
                    {
                      "property": "ShipVia_FK",
                      "sortProperty": "TblGnShipVium.ShipVia_ID",
                      "filterProperty": "TblGnShipVium.ShipVia_ID",
                      "template": "${data.TblGnShipVium?.ShipVia_ID}",
                      "title": "Tbl Gn Ship Vium"
                    },
                    {
                      "property": "PaymentTerm_FK",
                      "sortProperty": "TblGnPaymentTerm.PaymentTerm_ID",
                      "filterProperty": "TblGnPaymentTerm.PaymentTerm_ID",
                      "template": "${data.TblGnPaymentTerm?.PaymentTerm_ID}",
                      "title": "Tbl Gn Payment Term"
                    },
                    {
                      "property": "PaymentType_FK",
                      "sortProperty": "TblGnPaymentType.PaymentType_ID",
                      "filterProperty": "TblGnPaymentType.PaymentType_ID",
                      "template": "${data.TblGnPaymentType?.PaymentType_ID}",
                      "title": "Tbl Gn Payment Type"
                    },
                    {
                      "property": "Buyer_FK",
                      "sortProperty": "TblGnAddressBook1.AddressBook_ID",
                      "filterProperty": "TblGnAddressBook1.AddressBook_ID",
                      "template": "${data.TblGnAddressBook1?.AddressBook_ID}",
                      "title": "Tbl Gn Address Book"
                    },
                    {
                      "property": "TaxYN",
                      "title": "Tax YN"
                    },
                    {
                      "property": "TaxtRate",
                      "title": "Taxt Rate"
                    },
                    {
                      "property": "Department_FK",
                      "title": "Department FK"
                    },
                    {
                      "property": "PO_Amount",
                      "title": "PO Amount"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "bubble": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "error": [
                                  {
                                    "detail": "Unable to delete TblPoPurchaseOrder",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "CanERP_DB_AT132.deleteTblPoPurchaseOrder",
                                "parameters": [
                                  {
                                    "name": "PO_ID",
                                    "value": "${data.PO_ID}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "itemType": "ErpCan.Models.CanErpDbAt132.TblPoPurchaseOrder"
                },
                {
                  "name": "heading1",
                  "size": "H1",
                  "text": "TblPoPurchaseOrderDetails",
                  "type": "heading"
                },
                {
                  "icon": "add_circle_outline",
                  "name": "childAddButton",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-tbl-po-purchase-order-detail",
                        "type": "popup",
                        "parameters": [
                          {
                            "name": "PO_FK",
                            "value": "${master.PO_ID}"
                          }
                        ],
                        "then": [
                          {
                            "code": "grid1.Reload();",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid1",
                  "data": "${TblPoPurchaseOrderDetails}",
                  "columns": [
                    {
                      "property": "PO_FK",
                      "sortProperty": "TblPoPurchaseOrder.POStatus_FK",
                      "filterProperty": "TblPoPurchaseOrder.POStatus_FK",
                      "template": "${data.TblPoPurchaseOrder?.POStatus_FK}",
                      "title": "Tbl Po Purchase Order"
                    },
                    {
                      "property": "Inventory_FK",
                      "sortProperty": "TblIcInventory.Warehouse_FK",
                      "filterProperty": "TblIcInventory.Warehouse_FK",
                      "template": "${data.TblIcInventory?.Warehouse_FK}",
                      "title": "Tbl Ic Inventory"
                    },
                    {
                      "property": "Unit_Price",
                      "title": "Unit Price"
                    },
                    {
                      "property": "QtyOrdered",
                      "title": "Qty Ordered"
                    },
                    {
                      "property": "QtyReceived",
                      "title": "Qty Received"
                    },
                    {
                      "property": "SubTotal",
                      "title": "Sub Total"
                    },
                    {
                      "property": "TaxRate",
                      "title": "Tax Rate"
                    },
                    {
                      "property": "DiscountPercent",
                      "title": "Discount Percent"
                    },
                    {
                      "property": "Discount",
                      "title": "Discount"
                    },
                    {
                      "property": "Tax",
                      "title": "Tax"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "childGridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "error": [
                                  {
                                    "detail": "Unable to delete TblPoPurchaseOrder",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "CanERP_DB_AT132.deleteTblPoPurchaseOrderDetail",
                                "parameters": [
                                  {
                                    "name": "PO_FK",
                                    "value": "${data.PO_FK}"
                                  },
                                  {
                                    "name": "Inventory_FK",
                                    "value": "${data.Inventory_FK}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "grid1.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "events": {
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "PO_FK",
                            "value": "${event.PO_FK}"
                          },
                          {
                            "name": "Inventory_FK",
                            "value": "${event.Inventory_FK}"
                          }
                        ],
                        "path": "edit-tbl-po-purchase-order-detail",
                        "type": "popup",
                        "then": [
                          {
                            "code": "grid1.Reload();",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "itemType": "ErpCan.Models.CanErpDbAt132.TblPoPurchaseOrderDetail"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "CanERP_DB_AT132.getTblPoPurchaseOrders",
        "parameters": [],
        "then": [
          {
            "name": "getTblPoPurchaseOrdersResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<ErpCan.Models.CanErpDbAt132.TblPoPurchaseOrder>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Tbl PO List",
  "layout": "Main"
}