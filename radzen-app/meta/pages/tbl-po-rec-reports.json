        "parameters": [],
        "then": [
          {
            "name": "getTblPoRecReportsResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<ErpCan.Models.CanErpDbAt132.TblPoRecReport>"
