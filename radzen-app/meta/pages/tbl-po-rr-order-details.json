{
  "components": [
    {
      "type": "content",
      "container": "contentContainer0",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Tbl Po Rr Order Details"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-tbl-po-rr-order-detail",
                        "type": "popup",
                        "then": [
                          {
                            "code": "grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getTblPoRrOrderDetailsResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "RR_FK",
                            "value": "${event.RR_FK}"
                          },
                          {
                            "name": "Inventory_FK",
                            "value": "${event.Inventory_FK}"
                          }
                        ],
                        "path": "edit-tbl-po-rr-order-detail",
                        "type": "popup",
                        "argType": "ErpCan.Models.CanErpDbAt132.TblPoRrOrderDetail",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "RR_FK",
                      "sortProperty": "TblPoRecReport.PO_ID",
                      "filterProperty": "TblPoRecReport.PO_ID",
                      "template": "${data.TblPoRecReport?.PO_ID}",
                      "title": "Tbl Po Rec Report",
                      "type": "string"
                    },
                    {
                      "property": "Inventory_FK",
                      "sortProperty": "TblIcInventory.Warehouse_FK",
                      "filterProperty": "TblIcInventory.Warehouse_FK",
                      "template": "${data.TblIcInventory?.Warehouse_FK}",
                      "title": "Tbl Ic Inventory",
                      "type": "string"
                    },
                    {
                      "format": "decimal",
                      "property": "Unit_Price",
                      "title": "Unit Price",
                      "type": "number"
                    },
                    {
                      "format": "double",
                      "property": "QtyOrdered",
                      "title": "Qty Ordered",
                      "type": "number"
                    },
                    {
                      "format": "double",
                      "property": "QtyReceived",
                      "title": "Qty Received",
                      "type": "number"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "bubble": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "error": [
                                  {
                                    "detail": "Unable to delete TblPoRrOrderDetail",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "CanERP_DB_AT132.deleteTblPoRrOrderDetail",
                                "parameters": [
                                  {
                                    "name": "RR_FK",
                                    "value": "${data.RR_FK}"
                                  },
                                  {
                                    "name": "Inventory_FK",
                                    "value": "${data.Inventory_FK}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke",
                                "argType": "ErpCan.Models.CanErpDbAt132.TblPoRrOrderDetail"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "itemType": "ErpCan.Models.CanErpDbAt132.TblPoRrOrderDetail"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "CanERP_DB_AT132.getTblPoRrOrderDetails",
        "parameters": [],
        "then": [
          {
            "name": "getTblPoRrOrderDetailsResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<ErpCan.Models.CanErpDbAt132.TblPoRrOrderDetail>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Tbl Po Rr Order Details",
  "layout": "Main"
}