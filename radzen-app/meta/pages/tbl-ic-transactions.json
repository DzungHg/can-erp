{
  "components": [
    {
      "type": "content",
      "container": "contentContainer0",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Tbl Ic Transactions"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-tbl-ic-transaction",
                        "type": "popup",
                        "then": [
                          {
                            "code": "grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "Trans_No",
                            "value": "${event.Trans_No}"
                          }
                        ],
                        "path": "edit-tbl-ic-transaction",
                        "type": "popup",
                        "argType": "ErpCan.Models.CanErpDbAt132.TblIcTransaction",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "Trans_No",
                      "title": "Trans No",
                      "type": "string"
                    },
                    {
                      "property": "Trans_Code",
                      "sortProperty": "TblIcTransactionType.Transaction_Name",
                      "filterProperty": "TblIcTransactionType.Transaction_Name",
                      "template": "${data.TblIcTransactionType?.Transaction_Name}",
                      "title": "Tbl Ic Transaction Type",
                      "type": "string"
                    },
                    {
                      "format": "date-time",
                      "property": "Trans_Date",
                      "formatString": "{0:MM/dd/yyyy}",
                      "title": "Trans Date",
                      "type": "string"
                    },
                    {
                      "property": "Warehouse_ID",
                      "sortProperty": "TblIcWarehouse.WarehouseName",
                      "filterProperty": "TblIcWarehouse.WarehouseName",
                      "template": "${data.TblIcWarehouse?.WarehouseName}",
                      "title": "Tbl Ic Warehouse",
                      "type": "string"
                    },
                    {
                      "property": "Item_FK",
                      "sortProperty": "TblGnProduct.Product_ID",
                      "filterProperty": "TblGnProduct.Product_ID",
                      "template": "${data.TblGnProduct?.Product_ID}",
                      "title": "Tbl Gn Product",
                      "type": "string"
                    },
                    {
                      "property": "Reference_No",
                      "title": "Reference No",
                      "type": "string"
                    },
                    {
                      "format": "date-time",
                      "property": "Reference_Date",
                      "formatString": "{0:MM/dd/yyyy}",
                      "title": "Reference Date",
                      "type": "string"
                    },
                    {
                      "property": "Taxable",
                      "title": "Taxable",
                      "type": "boolean"
                    },
                    {
                      "property": "Cust_Vend_WarID",
                      "title": "Cust Vend War ID",
                      "type": "string"
                    },
                    {
                      "format": "double",
                      "property": "Trans_Qty",
                      "title": "Trans Qty",
                      "type": "number"
                    },
                    {
                      "property": "UOM",
                      "title": "UOM",
                      "type": "string"
                    },
                    {
                      "format": "decimal",
                      "property": "Trans_Amount",
                      "title": "Trans Amount",
                      "type": "number"
                    },
                    {
                      "format": "double",
                      "property": "Qty_On_Hand_EOB",
                      "title": "Qty On Hand EOB",
                      "type": "number"
                    },
                    {
                      "format": "double",
                      "property": "Qty_On_Order_EOB",
                      "title": "Qty On Order EOB",
                      "type": "number"
                    },
                    {
                      "format": "double",
                      "property": "Qty_On_Allowed_EOB",
                      "title": "Qty On Allowed EOB",
                      "type": "number"
                    },
                    {
                      "property": "GL_Asset",
                      "title": "GL Asset",
                      "type": "string"
                    },
                    {
                      "property": "GL_COGS",
                      "title": "GL COGS",
                      "type": "string"
                    },
                    {
                      "property": "GL_Sales",
                      "title": "GL Sales",
                      "type": "string"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "bubble": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "error": [
                                  {
                                    "detail": "Unable to delete TblIcTransaction",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "CanERP_DB_AT132.deleteTblIcTransaction",
                                "parameters": [
                                  {
                                    "name": "Trans_No",
                                    "value": "${data.Trans_No}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke",
                                "argType": "ErpCan.Models.CanErpDbAt132.TblIcTransaction"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "itemType": "ErpCan.Models.CanErpDbAt132.TblIcTransaction"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": []
  },
  "name": "Tbl Ic Transactions",
  "layout": "Main"
}