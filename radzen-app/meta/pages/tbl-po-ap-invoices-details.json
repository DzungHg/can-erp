                      "filterProperty": "TblIcUnit.UnitText",
                      "template": "${data.TblIcUnit?.UnitText}",
                      "title": "Tbl Ic Unit",
                      "type": "string"
                    },
                    {
                      "format": "decimal",
                      "property": "Unit_Price",
                      "title": "Unit Price",
                      "type": "number"
                    },
                    {
                      "format": "double",
                      "property": "Qty_Ordered",
                      "title": "Qty Ordered",
                      "type": "number"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "bubble": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "error": [
                                  {
                                    "detail": "Unable to delete TblPoApInvoicesDetail",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "CanERP_DB_AT132.deleteTblPoApInvoicesDetail",
                                "parameters": [
                                  {
                                    "name": "Vendor_ID",
                                    "value": "${data.Vendor_ID}"
                                  },
                                  {
                                    "name": "Invoice_No",
                                    "value": "${data.Invoice_No}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke",
                                "argType": "ErpCan.Models.CanErpDbAt132.TblPoApInvoicesDetail"
                              }
                            ]
                          }
                        }
                      ]
                    }
