{
  "components": [
    {
      "type": "content",
      "container": "contentContainer0",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Tbl Po Ap Invoices Details"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-tbl-po-ap-invoices-detail",
                        "type": "popup",
                        "then": [
                          {
                            "code": "grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "Vendor_ID",
                            "value": "${event.Vendor_ID}"
                          },
                          {
                            "name": "Invoice_No",
                            "value": "${event.Invoice_No}"
                          }
                        ],
                        "path": "edit-tbl-po-ap-invoices-detail",
                        "type": "popup",
                        "argType": "ErpCan.Models.CanErpDbAt132.TblPoApInvoicesDetail",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "Vendor_ID",
                      "sortProperty": "TblPoVendor.LastName",
                      "filterProperty": "TblPoVendor.LastName",
                      "template": "${data.TblPoVendor?.LastName}",
                      "title": "Tbl Po Vendor",
                      "type": "string"
                    },
                    {
                      "property": "Invoice_No",
                      "title": "Invoice No",
                      "type": "string"
                    },
                    {
                      "format": "date-time",
                      "property": "Invoice_Date",
                      "formatString": "{0:MM/dd/yyyy}",
                      "title": "Invoice Date",
                      "type": "string"
                    },
                    {
                      "property": "Inventory_FK",
